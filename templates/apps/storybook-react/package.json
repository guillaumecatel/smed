{
  "name": "template-app-storybook-react",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "storybook build",
    "clean": "shx rm -rf {node_modules,.turbo,storybook-static,coverage,tsconfig.tsbuildinfo,playwright-report,test-results}",
    "dev": "storybook dev --no-open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "pnpm exec playwright install",
    "start": "node ./dist/server/entry.mjs",
    "storybook": "storybook",
    "test:e2e": "pnpm exec playwright test",
    "test:integration": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@inlang/paraglide-js": "2.9.1",
    "@playwright/test": "1.58.0",
    "@storybook/addon-a11y": "10.2.0",
    "@storybook/addon-docs": "10.2.0",
    "@storybook/react-vite": "10.2.0",
    "@tailwindcss/vite": "4.1.18",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.18",
    "eslint-config": "workspace:*",
    "eslint-config-react": "workspace:*",
    "eslint-config-storybook": "workspace:*",
    "playwright": "1.58.0",
    "storybook": "10.2.0",
    "tailwindcss": "4.1.18",
    "tailwindcss-config": "workspace:*",
    "typescript": "5.9.3",
    "typescript-config": "workspace:*",
    "vite": "7.3.1",
    "vite-tsconfig-paths": "6.0.5",
    "vitest": "4.0.18"
  }
}
